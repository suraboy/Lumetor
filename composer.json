{
  "name": "boynii/lumen-generator",
  "description": "For lumen rest api with repository patten",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "boy",
      "email": "sirichai.jan@ascendcorp.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.3.0",
    "illuminate/support": ">=5.3.0",
    "guzzlehttp/guzzle": ">=5.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-0": {
      "Boynii\\LumenGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Boynii\\LumenGenerator\\Tests\\": "tests"
    }
  }
}
